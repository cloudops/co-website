  color: #005D91; }

a.nav-link.language-menu.list-group-item.list-group-item-action {
  display: inline;
  text-decoration: none;
  background-color: transparent;
  border: none; }

a.nav-link.language-menu.list-group-item.list-group-item-action:hover {
  text-decoration: none;
  background-color: transparent;
  border: none;
  font-weight: bold; }

a.nav-link.language-menu.list-group-item.list-group-item-action:focus {
  text-decoration: none;
  background-color: transparent;
  border: none;
  font-weight: bold; }

a.active {
  font-weight: bold; }

.navbar-card .navbar .navbar-nav .nav-item .language-menu .list-group-item .list-group-item-action:active {
  text-decoration: none;
  font-weight: bold;
  background-color: transparent;
  border: none;
  font-weight: bold; }

.navbar-card .navbar .navbar-nav .nav-item a.nav-link.language-menu .list-group-item .list-group-item-action {
  padding: 2px;
  display: inline; }

.navbar-card .navbar .nav-item .list-group .list-group-flush {
  display: inline;
