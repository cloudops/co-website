  font-size: 18px;
  line-height: 1.4; }

.card .corner-triangle {
  position: absolute;
  bottom: 12px;
  right: 12px; }

.corner-triangle {
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 0 0 20px 20px;
  border-color: transparent transparent #62B5E5 transparent; }

.card:hover {
  cursor: pointer; }

.card:hover .corner-triangle {
  position: absolute;
  bottom: 7px;
  right: 7px;
  transition-duration: 0.4; }

a:link {
